<UserControl x:Class="ZhApi.WpfApp.LogItems.VersionControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ZhApi.WpfApp.LogItems"
             xmlns:zhapi="clr-namespace:ZhApi.Cores;assembly=ZhApi.Base"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance Type=zhapi:VersionHelper}"
             Visibility="{Binding UpdateUrl,Converter={StaticResource notDefautVisibility}}"
             >
    <Button  Foreground="DarkOrange"
            Content="{Binding UpdatePrompt}"
            Command="{Binding ToUrlCommand}" />
</UserControl>
